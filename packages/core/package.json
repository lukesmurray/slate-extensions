{
  "name": "@slate-extensions/core",
  "author": "Luke Murray",
  "version": "0.3.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "module": "dist/core.esm.js",
  "size-limit": [
    {
      "path": "dist/core.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/core.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@slate-extensions/common": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.2",
    "slate": "^0.60.4",
    "slate-history": "^0.60.4",
    "slate-hyperscript": "^0.60.4",
    "slate-react": "^0.60.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "react": ">=17",
    "slate": "^0.60.4",
    "slate-history": "^0.60.4",
    "slate-hyperscript": "^0.60.4",
    "slate-react": "^0.60.4"
  }
}
